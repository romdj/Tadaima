		"autoprefixer": "^10.4.21",
		"eslint-plugin-svelte": "^2.46.1",
		"happy-dom": "^18.0.1",
		"postcss": "^8.5.6",
		"svelte": "^5.36.16",
		"svelte-check": "^4.3.0",
